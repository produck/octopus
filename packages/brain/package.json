		"rollup": "^3.17.2",
		"supertest": "^6.3.3"
	},
	"dependencies": {
		"@produck/duck": "^1.0.4",
		"@produck/duck-cli": "^1.2.2",
		"@produck/duck-cli-commander": "^1.0.1",
