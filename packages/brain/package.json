		"@produck/mold": "^0.3.1",
		"@produck/quack": "^0.1.1",
		"@produck/shop": "^0.2.0",
		"@types/node": "^18.14.0",
		"@types/semver": "^7.3.13",
		"koa-body": "^6.0.1",
		"semver": "^7.3.8"
